{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:electron": "vite",
    "build": "vue-tsc -b && vite build",
    "build:electron-main": "tsc -p tsconfig.electron.json",
    "build:electron": "npm run build && npm run build:electron-main && electron-builder",
    "preview": "vite preview"
  },
    "build": {
      "appId": "com.constella.app",
      "productName": "Constella",
      "directories": {
        "output": "dist-electron"
      },
      "files": [
        "dist/**",
        "electron/dist/**",
        "electron/**",
        "../server/dist/**"
      ],
      "extraResources": [
        {
          "from": "../server/dist",
          "to": "server"
        }
      ],
      "win": {
        "target": "nsis"
      }
    },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@types/dompurify": "^3.0.5",
    "@types/katex": "^0.16.8",
    "@types/markdown-it": "^14.1.2",
    "dompurify": "^3.3.1",
    "electron-store": "^11.0.2",
    "element-plus": "^2.13.0",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.27",
    "konva": "^10.0.12",
    "lib0": "^0.2.117",
    "markdown-it": "^14.1.0",
    "mermaid": "^11.12.2",
    "tdesign-icons-vue-next": "^0.4.1",
    "vue": "^3.5.26",
    "vue-i18n": "^9.14.4",
    "vue-konva": "^3.3.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.29"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vue-tsc": "^3.2.2",
    "wait-on": "^9.0.3"
  }
}